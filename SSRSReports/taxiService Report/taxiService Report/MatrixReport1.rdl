    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT FactTripDetail.FareAmount, FactTripDetail.GratuityAmount, FactTripDetail.ExtraFareAmount, FactTripDetail.TotalExtraAmount, FactTripDetail.TotalAmount, FactTripDetail.SurchargeAmount, FactTripDetail.PaymentType, 
                  DimCustomer.Title, DimCustomer.FirstName, DimCustomer.LastName, DimCustomer.Region, DimCustomer.Gender, DimDate.Year, DimDate.MonthName, DimDriver.FirstName AS Expr1, DimDriver.LastName AS Expr2, 
                  DimDriver.Title AS Expr3, DimPaymentType.name, DimVehicle.VehicleType, DimVehicle.VehicleModel, DimVehicle.VehicleModelYear, DimVehicle.VehicleColor
FROM     FactTripDetail INNER JOIN
                  DimCustomer ON FactTripDetail.CustomerId = DimCustomer.CustomerID INNER JOIN
                  DimDriver ON FactTripDetail.DriverId = DimDriver.DriverId INNER JOIN
                  DimVehicle ON FactTripDetail.VehicleId = DimVehicle.VehicleId INNER JOIN
                  DimDate ON FactTripDetail.StartDate = DimDate.DateKey CROSS JOIN
                  DimPaymentType</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
